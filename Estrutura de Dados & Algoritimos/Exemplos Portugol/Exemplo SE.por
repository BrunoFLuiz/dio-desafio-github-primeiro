programa
{
	
	funcao inicio()
	{
		escreva("1 - Abrir Netflix 2 - Abrir Amazon Prime 3 - Abrir HBO GO 4 - Sair")
		inteiro menu = 0
		escreva("\n" + "Sua Opção:")
		leia(menu)

		se (menu==1) {
			escreva("Ok, abrindo netflix!")
		}
		se (menu==2) {
			escreva("Ok, abrindo Amazon Prime!")
		}

		se (menu==3) {
			escreva("Ok, abrindo HBO GO!")
		}

		se (menu==4) {
			escreva("Ok, saindo do menu.")
		}
	}
}
